{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "nodejs -t",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/tools/clues.js",
            "args": [ "-pf.72", "-t",
                "fir"
        //       ,"-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nodejs 2,2 xor",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "localRoot": "${workspaceFolder}",
            "program": "${workspaceFolder}/src/tools/clues.js",
            "args": [ "clues.js", "-pf.72", "-c2,2", "-x2", "-v"
//                "--xor", "us 50",
                "--xor", "koa",
                "--xor", "21",
//                "--xor", "currant",
            ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "cppdbg -t",
            "program": "/usr/local/bin/node",
            "cwd": "${workspaceFolder}/src/tools",
            "args": [ "clues.js", "-pf.72", "-t", "fir"  ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "cppdbg 2,2 no --xor",
            "program": "/usr/local/bin/node",
            "cwd": "${workspaceFolder}/src/tools",
            "args": [ "clues.js", "-pf.72", "-c2,2", "-x2", "-v" ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "cpp -t",
            "program": "/usr/local/bin/node",
            "cwd": "${workspaceFolder}/src/tools",
            "args": [ "clues.js", "-pf.72", "-v", "-z2", "-t", "vacuum" ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "cppdbg xor",
            "program": "/usr/local/bin/node",
            "cwd": "${workspaceFolder}/src/tools",
            "args": [ "clues.js", "-pf.72", "-c2,2",
//                "--xor", "us 50",
                "--xor", "koa",
                "--xor", "21",
                "--or", "tree",
                "-v"
            ]
        }
    ]
}